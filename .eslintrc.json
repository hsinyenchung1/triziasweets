{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "react/jsx-no-bind": 0,
      "react/forbid-prop-types": 0,
      "react/no-array-index-key": 0

      
    }
  }
  